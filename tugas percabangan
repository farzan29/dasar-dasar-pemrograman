#include <iostream>
using namespace std;

int main()
{
  int x, y, z;
  cout << "masukkan nilai x :";
  cin >> x;
  cout << "masukkan nilai y :";
  cin >> y;
  z= x+y;
  cout << x << "+" << y << "-" << z << endl;
  if (z>=0) {
    cout << "nilai" << z << "adalah nilai positif" <<endl;
  }else if (z<0) {
    cout << "nilai" << z << "adalah nilai negatif" <<endl;
  }
  cout << "program ini adalah program berhitung" << endl;
  
  return 0;
  }
